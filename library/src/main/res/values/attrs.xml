<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--水印参数-->
    <declare-styleable name="WaterMarkView">
        <!--水印文字-->
        <attr name="wm_text" format="string" />
        <!--水平间距-->
        <attr name="wm_hSpace" format="dimension" />
        <!--垂直间距-->
        <attr name="wm_vSpace" format="dimension" />
        <!--角度-->
        <attr name="wm_degree" format="float" />
        <!--背景颜色-->
        <attr name="wm_bgColor" format="color" />
        <!--文字颜色-->
        <attr name="wm_textColor" format="color" />
        <!--文字大小-->
        <attr name="wm_textSize" format="dimension" />
    </declare-styleable>
</resources>